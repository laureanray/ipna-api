{
  "name": "ipna",
  "version": "1.0.0",
  "description": "package name availability checker that checks npm, github and other package repositories",
  "main": "src/server.js",
  "scripts": {
    "test": "yarn std && tap -Rspec",
    "testwatch": "tap -Rspec -w",
    "posttest": "tap --coverage-report=json src/tests/**.test.js --no-browser",
    "serve": "node src/server.js",
    "std": "standard --env mocha"
  },
  "repository": "https://github.com/laureanray/ipna",
  "author": "Laurean Ray Bahala",
  "license": "MIT",
  "dependencies": {
    "dotenv": "^8.2.0",
    "fastify": "^3.8.0",
    "got": "^11.8.0",
    "pino-pretty": "^4.3.0",
    "should": "^13.2.3",
    "uuid": "^8.3.1"
  },
  "devDependencies": {
    "fastify-swagger": "^3.5.0",
    "tap": "^14.10.8"
  }
}
